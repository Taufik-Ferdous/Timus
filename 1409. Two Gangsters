#include<bits/stdc++.h>
using namespace std;

#define optimize() ios_base::sync_with_stdio(0);cin.tie(0);cout.tie(0);

#define ll long long

//====================================

void solve(){
	ll a, b; cin>>a >>b;
	ll ans = a+b-1;
	cout<<ans-a <<' ' <<ans-b <<'\n';
}


int main(){
	optimize();

	ll t=1;
	// cin>>t;
	while(t--)
		solve();
}
